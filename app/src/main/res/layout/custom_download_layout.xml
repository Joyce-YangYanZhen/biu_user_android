<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent" android:layout_height="wrap_content"
              android:orientation="vertical">
    <RelativeLayout
        android:layout_width="@dimen/m314"
        android:layout_height="@dimen/m320">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/m291"
            android:background="@drawable/card_update"
            android:orientation="horizontal">

            <com.noplugins.keepfit.userplatform.util.ui.progress.CustomHorizontalProgresWithNum
                android:id="@+id/pb"
                android:layout_width="match_parent"
                android:layout_height="@dimen/m15"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginRight="@dimen/m20"
                android:layout_marginBottom="@dimen/m80"
                android:paddingLeft="0dp"
                android:paddingRight="0dp"
                android:progress="0"
                app:HorizontalProgresReachColor="#89ccde"
                app:HorizontalProgresTextColor="#000000"
                app:HorizontalProgresUnReachColor="#f2f9fb"/>

            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:textSize="@dimen/sp13"
                      android:layout_centerHorizontal="true"
                      android:layout_alignParentBottom="true"
                      android:textColor="@color/top_heiziti"
                      android:layout_marginBottom="@dimen/m20"
                      android:text="@string/tv228"/>
        </RelativeLayout>

        <ImageView
            android:layout_width="@dimen/m25"
            android:layout_height="@dimen/m25"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/icon_cancel"/>

        <LinearLayout
            android:id="@+id/versionchecklib_version_dialog_cancel"
            android:layout_width="@dimen/m50"
            android:layout_height="@dimen/m50"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:background="@color/transparent"
            android:gravity="center"
            android:orientation="horizontal"/>
    </RelativeLayout>

